                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End banner Area -->
    <!-- start features Area -->
    <section class="features-area section_gap">
        <div class="container">
            <div class="row features-inner">
                <!-- single features -->
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="single-features">
                        <div class="f-icon">
                            <img src="~/img/features/f-icon1.png" alt="">
                        </div>
                        <h6>@ShoesProject.Resources.Data.Delivery</h6>
                        <p>@ShoesProject.Resources.Data.Content</p>
                    </div>
                </div>
                <!-- single features -->
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="single-features">
                        <div class="f-icon">
                            <img src="~/img/features/f-icon2.png" alt="">
                        </div>
                        <h6> @ShoesProject.Resources.Data.Policy</h6>
                        <p>@ShoesProject.Resources.Data.Content</p>
                    </div>
                </div>
                <!-- single features -->
                <div class="col-lg-3 col-md-6 col-sm-6">

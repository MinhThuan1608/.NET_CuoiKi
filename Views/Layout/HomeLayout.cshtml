                        <ul class="nav navbar-nav menu_nav ml-auto">

                            <li class="nav-item">
                                <a class="nav-link" href="https://www.facebook.com/nguyenson.nguyen.12576"
                                   href="index.jsp"><i class="fab fa-facebook"></i><span class="text" style="color: white;"> Facebook</span></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link"
                                   href="index.jsp"><i class="fab fa-instagram"></i><span class="text" style="color: white;"> Instagram</span></a>
                            </li>
                        </ul>
                        <p style="color: white;">@ShoesProject.Resources.Data.ContentFooter</p>
